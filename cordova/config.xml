<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hayne.angular2typescriptwebpack" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Angular2TypeScriptWebpack</name>
    <description>
        A sample Apache Cordova application using Angular 2, TypeScript and Webpack.
    </description>
    <author email="thorgal@hayne.com" href="http://www.hayne.com">
        Thorgal Hayne
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
</widget>
